{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "types": [
            "cypress",
            "node"
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./cypress/*"
            ]
        }
    },
    "include": [
        "cypress/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}